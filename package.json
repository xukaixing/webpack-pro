{
  "name": "webpack-pro",
  "version": "1.0.7",
  "description": "webpack-pro",
  "bundlesize":[
    {
      "path": "./dist/assets/img/*.jpg",
      "maxSize": "100 kB"
    },
    {
      "path": "./dist/assets/js/*.js",
      "maxSize": "50 kB"
    },
    {
      "path": "./dist/assets/js/app.vendors.*.js",
      "maxSize": "50 kB"
    }
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.js",
    "analyze": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "checksize": "bundlesize"
  },
  "keywords": [
    "webpack",
    "webpack-cli",
    "webpack-dev-server",
    "eslint",
    "stylelint",
    "prettier",
    "autoprefixer",
    "babel",
    "postcss",
    "sass/less",
    "terser-webpack-plugin",
    "html-webpack-plugin",
    "mini-css-extract-plugin",
    "optimize-css-assets-webpack-plugin",
    "clean-webpack-plugin",
    "cross-en"
  ],
  "author": "xukaixing<andy.ten@tom.com>",
  "license": "ISC",
  "browserslist": [
    "defaults",
    "not ie <= 10",
    "last 2 versions",
    "> 1%"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/runtime": "^7.13.17",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.11.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.11",
    "ejs-loader": "^0.5.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-commonjs": "^1.0.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.9.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^1.4.5",
    "url-loader": "^4.1.1",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  }
}
